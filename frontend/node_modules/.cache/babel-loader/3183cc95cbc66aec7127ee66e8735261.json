CAAC,MAAd,CAAnB;AACA,QAAA,IAAI,IAAE,GAAN;AACA,QAAA,QAAQ,CAAC,eAAT,CAAyB,KAAzB,CAA+B,WAA/B,CAA2C,QAA3C,EAAoD,IAApD;AACA,QAAA,YAAY,CAAC,MAAb,GAAoB,IAApB;AAEA,aAAK,KAAL,CAAW,QAAX;AACA,aAAK,KAAL,CAAW,MAAX;AACA;AACD,OAXD,MAWK;AACH,aAAK,MAAL,GAAY,KAAK,KAAL,GAAW,sBAAX,GAAkC,KAAK,OAAL,CAAa,QAA/C,GAAwD,4BAApE;AACA,aAAK,KAAL,CAAW,QAAX;AACA,aAAK,KAAL,GAAW,EAAX;AACA,aAAK,MAAL,GAAY,KAAK,IAAjB;AACA,aAAK,SAAL,GAAe,MAAf;AACA,aAAK,SAAL,GAAe,KAAf;AACA,aAAK,KAAL,CAAW,MAAX,CAAkB,KAAlB;AACF;AAGD,KA5BK;AA6BN,IAAA,IA7BM,kBA6BA;AAAA;;AAEJ,WAAK,KAAL,CAAW,MAAX;AACA,WAAK,SAAL,GAAe,UAAf;AACA,WAAK,MAAL,GAAY,KAAK,EAAjB;AACA,WAAK,KAAL,GAAW,EAAX;AACA,WAAK,SAAL,GAAe,IAAf,CANI,CAOJ;;AACA,WAAK,SAAL,CAAe,YAAI;AACjB,QAAA,MAAI,CAAC,KAAL,CAAW,KAAX,CAAiB,KAAjB;AACD,OAFD;AAID,KAzCK;AA0CN,IAAA,KA1CM,iBA0CA,CA1CA,EA0CE;AACN,UAAI,CAAC,CAAC,GAAF,IAAO,OAAX,EAAmB;AACjB,aAAK,MAAL;AACD,OAFD,MAEK;AACH,aAAK,SAAL,GAAe,QAAf;AACF;AACD,KAhDK;AAkDN,IAAA,KAlDM,mBAkDC;AACL;AACA,WAAK,MAAL,GAAY,EAAZ;AAEF;AAtDM;AAzBI,CAAd","sourcesContent":["<template>\n  <div class=\"bubble\">\n    <div class=\"question\">\n      <h2>{{title}}</h2>\n      <h2>{{result}}</h2>\n\n      <input v-if=\"showInput\" ref=\"input\" class=\"test\" type=\"number\" v-model='input' @keyup=\"keyup\" >\n\n      <button class=\"test\" type=\"button\" name=\"button\" ref=\"action\" @click='action' >\n        <!-- <div class=\"go\"> -->\n          {{buttonMsg}}\n        <!-- </div> -->\n      </button>\n    </div>\n\n  </div>\n</template>\n\n<script>\n  export default{\n    props:[\n      'content',\n    ],\n    watch:{\n      content(newTitle){\n        this.title=newTitle.title\n      }\n    },\n    data(){\n      return {\n        title:this.content.title,\n        showInput:true,\n        result:\"\",\n        input:\"\",\n        buttonMsg:\"keine Ahnung\",\n      }\n    },\n    created(){\n      this.action=this.go\n      this.$nextTick(()=>{\n        this.$refs.input.focus()\n        console.log(\"input found:\",this.$refs.input);\n      })\n    },\n    methods:{\n      go(){\n        if (this.input==\"\"){\n\n          return\n        }\n        if (this.input==this.content.solution){\n          this.input=\"\"\n\n          var glow=parseFloat(localStorage.energy)\n          glow+=0.1\n          document.documentElement.style.setProperty('--glow',glow)\n          localStorage.energy=glow\n\n          this.$emit('solved')\n          this.$emit('next')\n          return\n        }else{\n          this.result=this.input+\" ist leider Falsch. \"+this.content.solution+' ist die richtige Antwort.'\n          this.$emit('failed')\n          this.input=\"\"\n          this.action=this.next\n          this.buttonMsg=\"next\"\n          this.showInput=false\n          this.$refs.action.focus()\n        }\n\n\n      },\n      next(){\n\n        this.$emit('next')\n        this.buttonMsg=\"aufgeben\"\n        this.action=this.go\n        this.input=\"\"\n        this.showInput=true\n        // console.log(this.$refs.input.childNodes[0]);\n        this.$nextTick(()=>{\n          this.$refs.input.focus()\n        })\n\n      },\n      keyup(k){\n        if (k.key==\"Enter\"){\n          this.action()\n        }else{\n          this.buttonMsg=\"fertig\"\n        }\n      },\n\n      start(){\n        // this.$refs.input.focus()\n        this.result=\"\"\n\n      }\n    },\n  }\n</script>\n"],"sourceRoot":""}]}