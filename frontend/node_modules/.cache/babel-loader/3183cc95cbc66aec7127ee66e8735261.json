s.next\n        this.buttonMsg=\"next\"\n        this.showInput=false\n        this.$refs.action.focus()\n\n      },\n      next(){\n        this.$emit('next')\n        this.buttonMsg=\"aufgeben\"\n        this.action=this.go\n        this.input=\"\"\n        this.showInput=true\n        // console.log(this.$refs.input.childNodes[0]);\n        this.$nextTick(()=>{\n          this.$refs.input.focus()\n        })\n\n      },\n      keyup(k){\n        if (k.key==\"Enter\"){\n          this.action()\n        }else{\n          this.buttonMsg=\"fertig\"\n        }\n      },\n\n      start(){\n        // this.$refs.input.focus()\n        this.result=\"\"\n\n      }\n    },\n  }\n</script>\n"],"sourceRoot":""}]}