AAnCY,CAAd","sourcesContent":["<template>\n  <div class=\"bubble\">\n    <div class=\"question\">\n\n      <h2>{{content.title}}</h2>\n      <input class=\"test\" type=\"number\" v-model='input' ref=\"input\" @keyup.enter='onEnter'>\n      <button class=\"test\" type=\"button\" name=\"button\" @click='go' >\n        <div class=\"go\">\n          GO\n        </div>\n      </button>\n      <h2>{{result}}</h2>\n    </div>\n\n  </div>\n</template>\n\n<script>\n  export default{\n    props:[\n      'content',\n    ],\n    data(){\n      return {\n        result:\"\",\n        input:\"\"\n      }\n    },\n    methods:{\n      go(){\n        if (this.input==\"\"){\n          return\n        }\n        if (this.input==this.content.solution){\n          this.result=this.content.solution+\" ist die richtig!\"\n          this.input=\"\"\n          this.$emit('solved')\n        }else{\n          this.result=this.input+\" ist leider Falsch. \"+this.content.solution+' ist die richtige Antwort.'\n          this.input=\"\"\n          this.$emit('failed')\n        }\n      },\n      onEnter(){\n        console.log(\"enter\")\n        this.go()\n      },\n      start(){\n        this.$refs.input.focus()\n        this.result=\"...\"\n      }\n    },\n    created(){\n    }\n  }\n</script>\n"],"sourceRoot":""}]}