e.log(k.key);\n        if (k.key==\"Enter\"){\n          this.action()\n        }else{\n          this.buttonMsg=\" ↵ fertig\"\n        }\n      },\n\n      start(){\n        this.$refs.input.focus()\n        this.result=\"...\"\n      }\n    },\n  }\n</script>\n"],"sourceRoot":""}]}